{
  "version": 2,
  "extra_config": {
    "github_com/devopsfaith/krakend-cors": {
      "allow_origins": [
        "*"
      ],
      "expose_headers": [
        "Content-Length"
      ],
      "max_age": "12h",
      "allow_methods": [
        "GET",
        "HEAD",
        "POST",
        "PUT",
        "DELETE",
        "CONNECT",
        "OPTIONS",
        "TRACE",
        "PATCH"
      ],
      "allow_credentials": true
    }
  },
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "name": "TemplateService",
  "endpoints": [
    {
      "endpoint": "/service-initial",
      "method": "GET",
      "extra_config": {
        "github.com/devopsfaith/krakend/proxy": {
          "sequential": false
        }
      },
      "output_encoding": "json",
      "concurrent_calls": 1,
      "querystring_params": [
        "name",
        "gender"
      ],
      "backend": [
        {
          "url_pattern": "/",
          "encoding": "json",
          "sd": "dns",
          "extra_config": {},
          "method": "GET",
          "host": [
            "localhost"
          ],
          "disable_host_sanitize": true
        }
      ]
    }
  ]
}